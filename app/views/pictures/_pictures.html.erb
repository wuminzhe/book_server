<%
   pictures = []
   @pictures.each do |picture|
     pictures << picture
     if pictures.length == 6
%>
       <%= render :partial => 'row', :locals => { pictures: pictures } %>
<%
       pictures = []
     end
   end
   if pictures.length > 0
%>
     <%= render :partial => 'row', :locals => { pictures: pictures } %>
<%
   end
%>